#include <iostream>
#include <bits/stdc++.h>
using namespace std;

void sort(int arr[], int n)
{
  int i,j,temp;
  for(i=0; i<=n-2;i++)
    {
      int mini = i;
      for(j=i; j<=n-1; j++){
        if(arr[j] < arr[mini])
          mini = j;
      }
      temp = arr[mini];
      arr[mini] = arr[i];
      arr[i] = temp;
    } 
}
int main(){
  int n;
  cin >> n;
  int arr[n];
  //input elements
  for(int i=0; i<n; i++)
    {
      cin >> arr[i];
    }
  //sort array
  sort(arr, n);
  // printArray(arr, n);
  for(int i=0; i<n; i++)
    {
      cout << arr[i] << " ";
    }
  }


